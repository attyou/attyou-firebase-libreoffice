{
  "name": "@attyou/firebase-libreoffice",
  "version": "2.2.9",
  "description": "85 MB LibreOffice to fit insidefirebase compressed with Brotli forked from  shelfio/aws-lambda-libreoffice ",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/attyou/firebase-libreoffice.git"
  },
  "author": "Vlad Holubiev <vlad@shelf.io> (shelf.io)",
  "engines": {
    "node": ">=8.10"
  },
  "scripts": {
    "build": "yarn build:types && babel src --out-dir lib --ignore '**/*.test.ts' --extensions '.js' --copy-files",
    "build:types": "tsc --emitDeclarationOnly --declaration --isolatedModules false --declarationDir lib",
    "coverage": "jest --coverage",
    "lint": "eslint . --fix",
    "prepack": "yarn build",
    "test": "jest src",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "keywords": [
    "aws lambda",
    "libreoffice",
    "serverless",
    "pdf"
  ],
  "dependencies": {
    "@shelf/aws-lambda-brotli-unpacker": "1.0.0",
    "@shelf/is-audio-filepath": "0.0.3",
    "del": "4.1.1",
    "is-image": "3.0.0",
    "is-video": "1.0.1"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-typescript": "7.3.3",
    "@shelf/eslint-config": "0.3.5",
    "@types/jest": "24.0.13",
    "@types/node": "8",
    "babel-jest": "24.8.0",
    "eslint": "5.16.0",
    "husky": "2.4.1",
    "jest": "24.8.0",
    "lint-staged": "8.2.0",
    "prettier": "1.18.2",
    "typescript": "3.8.3"
  },
  "resolutions": {
    "jest-diff": "25.2.3",
    "pretty-format": "24.9.0"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.{html,json,md,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/attyou/firebase-libreoffice/issues"
  },
  "homepage": "https://github.com/attyou/firebase-libreoffice#readme"
}
